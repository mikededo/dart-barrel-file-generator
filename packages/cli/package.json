{
  "name": "@dbfg/cli",
  "type": "module",
  "version": "1.3.0",
  "private": false,
  "description": "CLI to generate Dart barrel files",
  "bin": {
    "dbfg-cli": "./dist/bin.cjs"
  },
  "engines": {
